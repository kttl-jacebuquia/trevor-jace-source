{
    "name": "kettle/trevor-wp-plugin",
    "type": "wordpress-plugin",
    "license": "Private",
    "require": {
        "twig/twig": "^3.0",
        "monolog/monolog": "^2.1",
        "google/apiclient": "^2.7"
    },
    "config": {
        "vendor-dir": "lib",
        "autoloader-suffix": "TrevorWPPlugin",
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "psr-4": {
            "TrevorWP\\": "inc/"
        }
    },
	"extra": {
		"google/apiclient-services": [
			"AnalyticsReporting"
		]
	}
}
